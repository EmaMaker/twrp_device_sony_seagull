# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/system       					ext4		/dev/block/bootdevice/by-name/system    				flags=wipeingui
/cache     					ext4		/dev/block/bootdevice/by-name/cache					flags=wipeingui
/data     					ext4		/dev/block/bootdevice/by-name/userdata					flags=wipeingui
/boot  	    					emmc		/dev/block/bootdevice/by-name/boot 					flags=defaults
/recovery      					emmc	  	/dev/block/bootdevice/by-name/FOTAKernel				flags=defaults
/firmware      					vfat	   	/dev/block/bootdevice/by-name/modem  					flags=defaults
/external_sd					vfat		/devices/msm_sdcc.2/mmc_host*
#/usb-otg					vfat	   	/devices/platform/msm_hsusb_host/usb1/1-1/